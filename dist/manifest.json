{
  "manifest_version": 3,
  "name": "EMR Helper",
  "description": "Provide additional functionality for PT EMR ",
  "version": "0.0.1",
  "permissions": ["tabs"],
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://emr.webpt.com/*"],
      "js": ["script.js"]
    }
  ]
}
